{
   "name": "@node-graphql-jwt/backend",
   "version": "1.0.0",
   "scripts": {
      "start": "nodemon --exec ts-node src/index.ts",
      "entity:create": "ts-node --transpile-only ./node_modules/typeorm/cli.js entity:create -n",
      "migrate:generate": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:generate -n",
      "migrate:run": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run",
      "migrate:revert": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:revert",
      "schema:drop": "ts-node --transpile-only ./node_modules/typeorm/cli.js schema:drop"
   },
   "devDependencies": {
      "@types/bcryptjs": "^2.4.2",
      "@types/cookie-parser": "^1.4.2",
      "@types/cors": "^2.8.10",
      "@types/jsonwebtoken": "^8.5.1",
      "@types/node": "^14.14.35",
      "nodemon": "^2.0.7",
      "ts-node": "9.1.1",
      "typescript": "4.2.3"
   },
   "dependencies": {
      "@types/express": "^4.17.11",
      "@types/graphql": "^14.5.0",
      "apollo-server-express": "^2.21.2",
      "bcryptjs": "^2.4.3",
      "cookie-parser": "^1.4.5",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "graphql": "^15.5.0",
      "jsonwebtoken": "^8.5.1",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.10",
      "type-graphql": "^1.1.1",
      "typeorm": "0.2.31"
   }
}
